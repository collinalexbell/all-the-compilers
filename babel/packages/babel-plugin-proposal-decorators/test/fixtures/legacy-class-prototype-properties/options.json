{
  "assumptions": {
    "setPublicClassFields": true
  },
  "presets": [["env", { "targets": { "browsers": "ie 6" } }]],
  "plugins": [
    ["proposal-decorators", { "legacy": true }],
    ["babel-plugin-polyfill-es-shims", { "method": "usage-global", "targets": {
      "node": "current"
    }}]
  ]
}
