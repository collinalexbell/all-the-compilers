{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "lib": [
            "es2015"
        ],
        "noImplicitAny": true,
        "noEmitOnError": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "newLine": "LF",
        "strictNullChecks": true,
        "sourceMap": false
    },
    "exclude": [
        "node_modules"
    ]
}
