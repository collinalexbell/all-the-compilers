MODULE Pattern1;
IMPORT SYSTEM;
  VAR ch: CHAR;
      k,j: INTEGER;
      x: REAL;
      s: SET;
BEGIN
  ch := "0";
  WriteInt(ORD(ch));
  k := 10;
  WriteInt(k);
  k := -65536;
  WriteInt(k);
  k := -65537;
  WriteInt(k);
  k := 65535;
  WriteInt(k);
  k := 65536;
  WriteInt(k);
  k := 65537;
  WriteInt(k);
  x := 1.0;
  WriteReal(x);
  s := {0, 4, 8};
  WriteInt(SYSTEM.VAL(INTEGER, s));
  s := {3..5};
  WriteInt(SYSTEM.VAL(INTEGER, s));
  k := 5;
  s := {3..k};
  WriteInt(SYSTEM.VAL(INTEGER, s));
  k := 18;
  s := {16..k};
  WriteInt(SYSTEM.VAL(INTEGER, s));
  k := 3;
  s := {k..5};
  WriteInt(SYSTEM.VAL(INTEGER, s));
  k := 3;
  s := {k..15};
  WriteInt(SYSTEM.VAL(INTEGER, s));
  j := 5;
  s := {k..j};
  WriteInt(SYSTEM.VAL(INTEGER, s));
END Pattern1.