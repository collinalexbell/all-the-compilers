  Null
  SetL $foo
  PopC
  String "abc"
  SetL $baz
  PopC
  String "xyz"
  SetL $t
  PopC
  BaseL $baz Warn
  QueryM 0 CGet EL:$t Any
  Int 12
  BaseH
  Dim Define ET:"x" Any
  Dim Define ET:"y" Any
  Dim Define ET:"q" Any
  SetM 0 PC:1 Any
