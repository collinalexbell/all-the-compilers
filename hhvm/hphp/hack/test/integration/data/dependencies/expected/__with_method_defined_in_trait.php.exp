<?hh

interface IWibble {
  public function f(): void;
}

trait TWibble {
  require implements \IWibble;

  public function f(): void {
    throw new \Exception();
  }
}

class Wibble implements \IWibble {
  use \TWibble;
}

function with_method_defined_in_trait(IWibble $w, Wibble $_): void {
  $w->f();
}
