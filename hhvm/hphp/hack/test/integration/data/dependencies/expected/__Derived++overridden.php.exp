<?hh

abstract class AbstractBase {}

class ImplementingBase extends \AbstractBase {
  public function overridden(): int {
    throw new \Exception();
  }
}

final class Derived extends \ImplementingBase {
  private int $result;

  public function __construct(int $num) {
    throw new \Exception();
  }

  <<__Override>>
  public function overridden(): int {
    return $this->result;
  }
}
