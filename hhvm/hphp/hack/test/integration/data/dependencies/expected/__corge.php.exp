<?hh

interface ICorge {
  public function get(): this::Tthis;

  abstract const type Tthis as this;
}

final class Corge implements \ICorge {
  public function get(): this::Tthis {
    throw new \Exception();
  }

  const type Tthis = \Corge;
}

function corge(Corge $x, ICorge $y): void {
  $_ = $x->get();
  $_ = $y->get();
}
