(alias
    (name autocomplete_enum_class_label)
    (deps %{exe:../../../src/hh_single_complete.exe}
          %{project_root}/test/verify.py
          %{project_root}/test/review.sh
          (glob_files %{project_root}/test/autocomplete/enum_class_label/HH_FLAGS)
          (glob_files %{project_root}/test/autocomplete/enum_class_label/*.php)
          (glob_files %{project_root}/test/autocomplete/enum_class_label/*.exp))
    (action (run %{project_root}/test/verify.py %{project_root}/test/autocomplete/enum_class_label
    --program %{exe:../../../src/hh_single_complete.exe})))

(alias
    (name runtest)
    (deps (alias autocomplete_enum_class_label)))
